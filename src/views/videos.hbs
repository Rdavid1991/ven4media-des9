<div class="card mt-2 w-100">
    <div class="card-header bg-dark text-white">
        <h3> <i class="far fa-images"></i>Lista de imagenes</h3>
    </div>
    <div class="card-body {{url}}">
        <div class="row">
            {{#each videos}}
            <div class="col-md-2">
                <div id="preview" onclick="watchPreview('{{miniature}}')">
                    <img src="http://localhost:4000/video/miniature/{{miniature}}.png" alt="" class="w-10 h-10 img-thumbnail">
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>

{{>video-modal}}